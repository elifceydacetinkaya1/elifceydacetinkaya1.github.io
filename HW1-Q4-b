#include <stdio.h>

void computeHomeValue(void);
int main(void)
{
    computeHomeValue();
    return 0;
}
void computeHomeValue(void)
{
    int popularity;
    int size;
    int homeValue;
    printf("Enter popularity:");
    scanf("%d", &popularity);
    printf("\nEnter size:");
    scanf("%d",&size );
    homeValue = (popularity * popularity * popularity + size * size) * 10000;
    printf("\nHome value is:%d", homeValue);
    
   
    
}
